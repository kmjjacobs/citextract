[FORMAT]
max-line-length=120

[MASTER]
load-plugins = pylint.extensions.docparams
extension-pkg-whitelist=numpy,torch

[TYPECHECK]
ignored-modules=numpy,torch
ignored-classes=numpy,torch

[MESSAGES CONTROL]
disable=W0221,R0903,I1101,R0901,W0703,R0801,R0913

[BASIC]
good-names=name,project_settings,__version__
